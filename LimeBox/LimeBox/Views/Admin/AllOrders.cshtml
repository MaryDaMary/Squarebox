@model List<AdminAllOrdersVM>


<table id="OrderTable" class="table">
    <thead>
        <tr>
            <td style="width: 10%">Order id</td>
            <td style="width: 23%">Namn</td>
            <td style="width: 20%">Status</td>
            <td style="width: 15%">Order datum</td>
        </tr>
    </thead>
    @foreach (var item in Model)
    {
    <tbody>
        <tr onclick="SelectOrder(@item.Order.Id, this)" id="@item.Order.Id">
            <td>@item.Order.Id</td>
            <td>@item.Order.FirstName @item.Order.LastName</td>
            <td>@item.StatusName</td>
            <td>@item.Order.OrderDate.ToShortDateString()</td>
        </tr>
        <tr hidden>
            <td style="padding:0px" colspan="4">
                <div id="Content"></div>
            </td>
        </tr>
    </tbody>
    }
</table>
