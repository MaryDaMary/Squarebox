<!DOCTYPE html>

<html>
<head>
	<meta name="viewport" content="width=device-width" />
	<link rel="icon" href="https://i.imgur.com/agcRyxZ.png" />
	<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
	<title>@ViewBag.Title</title>
	<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
	<link href="~/Css/StyleSheet.min.css" rel="stylesheet" />

</head>
<body>
    <div id="wrap">
        <header class="headerDiv">
            <div class="logoDiv">
                <a href="/Home/">
                    <img class="Header" src="~/Images/SBLogoWhite.png" alt="loggan" />
                </a>
            </div>
            <div class="logInHeader">
                @await Component.InvokeAsync("Login")
            </div>
            <div style="padding-top: 34px">
                @await Component.InvokeAsync("CheckOut")
            </div>

        </header>
        @await Component.InvokeAsync("BoxNav")
        <div>
            @RenderBody()
        </div>
        <div style="clear : both"></div>
    </div>
    @Html.Partial("_Footer")
    <script src="https://code.jquery.com/jquery-3.3.1.min.js"
            integrity="sha256-FgpCb/KJQlLNfOu91ta32o/NMZxltwRo8QtmkMRdAu8="
            crossorigin="anonymous"></script>
    <script src="~/Script/JavaScript.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script> 
    <script src="http://ajax.aspnetcdn.com/ajax/jquery.validate/1.14.0/jquery.validate.min.js"></script>
    <script src="http://ajax.aspnetcdn.com/ajax/mvc/5.2.3/jquery.validate.unobtrusive.min.js"></script>
    <br />
</body>
</html>
